# NO2预测系统环境变量配置模板
# 复制此文件为.env并填入真实值

# 数据库配置
DATABASE_URL=mysql://username:password@hostname:3306/database_name

# 和风天气API配置
HF_API_HOST=your_api_host
HF_PRIVATE_KEY_FILE=ed25519-private.pem
HF_PROJECT_ID=your_project_id
HF_KEY_ID=your_credential_id

# AI模型配置（可选）
AI_API_KEY=sk-xxx  # 填入你的硅基流动API
AI_API_BASE=https://api.siliconflow.cn/v1
AI_MODEL_NAME=Qwen/Qwen3-14B